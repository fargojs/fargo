{
  "name": "@fargo/core",
  "version": "0.0.1",
  "description": "Fargo Core",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "repository": "https://github.com/fargojs/fargo/blob/main/packages/core",
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fargojs/fargo/issues"
  },
  "homepage": "https://github.com/fargojs/fargo/blob/main/packages/core#readme",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "dependencies": {
    "@fargo/config": "workspace:*",
    "debug": "^4.3.4",
    "fastify": "^4.2.0"
  },
  "devDependencies": {
    "@fargo/types": "workspace:*",
    "@types/debug": "^4.1.7"
  }
}
