{
  "name": "@fargo/fargo",
  "version": "0.0.1",
  "description": "A private registry for VSIX extensions.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "fargo": "./bin/fargo"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "repository": "https://github.com/fargojs/fargo/blob/main/packages/fargo",
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fargojs/fargo/issues"
  },
  "homepage": "https://github.com/fargojs/fargo/blob/main/packages/fargo#readme",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "dependencies": {
    "@fargo/cli": "workspace:*",
    "@fargo/core": "workspace:*"
  }
}
