{
  "name": "@zotera/types",
  "version": "0.0.1",
  "description": "zotera type definitions",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/zotera/zotera/blob/main/packages/types#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zotera/zotera",
    "directory": "packages/types"
  },
  "bugs": {
    "url": "https://github.com/zotera/zotera/issues"
  },
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.d.ts",
      "import": "./dist/types.d.ts"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "require": "./dist/api.d.ts",
      "import": "./dist/api.d.ts"
    }
  },
  "main": "./dist/types.d.ts",
  "module": "./dist/types.d.ts",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    "*": {
      "api": [
        "dist/api.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "lint": "eslint --cache {src,tests}/**/*.ts"
  }
}
