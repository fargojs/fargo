{
  "name": "@zotera/cli",
  "version": "0.0.1",
  "description": "",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "zotera": "./bin/zotera"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "repository": "https://github.com/zotera/zotera/blob/main/packages/cli",
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zotera/zotera/issues"
  },
  "homepage": "https://github.com/zotera/zotera/blob/main/packages/cli#readme",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "dependencies": {
    "@oclif/core": "^1.9.5",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-not-found": "^2.3.1",
    "@zotera/config": "workspace:*",
    "@zotera/core": "workspace:*",
    "@zotera/server": "workspace:*",
    "inquirer": "8.2.4",
    "yeoman-environment": "^3.9.1",
    "yeoman-generator": "^5.6.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.1",
    "@types/node": "^18.0.3",
    "@types/yeoman-environment": "^2.10.7",
    "@types/yeoman-generator": "^5.2.10",
    "esbuild-plugin-copy": "^1.3.0",
    "oclif": "^3.1.0"
  },
  "oclif": {
    "bin": "zotera",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "topics": {
      "plugins": {
        "description": "manage zotera community plugins"
      }
    }
  }
